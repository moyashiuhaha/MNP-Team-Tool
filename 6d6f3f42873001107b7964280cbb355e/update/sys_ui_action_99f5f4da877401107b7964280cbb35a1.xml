        <script><![CDATA[if (typeof window == 'undefined') {
    caluc_button();
}
function caluc_button(){
	var total_time = 0;
	var gr_tasks = new GlideRecord('x_547977_mnp_team_work_task');
	gr_tasks.addQuery('u_parent_day', current.sys_id);
	gr_tasks.addQuery('u_work_summary','!=', 100);
	gr_tasks.query();
	
	while(gr_tasks.next()){
		total_time += gr_tasks.u_hour;
	}
	current.u_total_time = total_time;
	action.setRedirectURL(current);
	current.update();
}]]></script>
